<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/CCSNA?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">123@esta!</property>
    <property name="hibernate.connection.datasource">java:comp/env/jdbc/ccsnaDB</property>
    <mapping resource="com/ccsna/safetynet/model/SuccessStory.hbm.xml"/>
    <mapping resource="com/ccsna/safetynet/model/News.hbm.xml"/>
    <mapping resource="com/ccsna/safetynet/model/Member.hbm.xml"/>
    <mapping resource="com/ccsna/safetynet/model/Links.hbm.xml"/>
    <mapping resource="com/ccsna/safetynet/model/Agency.hbm.xml"/>
    <mapping resource="com/ccsna/safetynet/model/County.hbm.xml"/>
    <mapping resource="com/ccsna/safetynet/model/Services.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
